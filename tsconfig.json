{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "es6",
    "lib": ["es2016", "dom"],
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "allowJs": true
  },
  "include": [
    "src/js/*"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}